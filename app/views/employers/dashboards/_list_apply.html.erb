<tr>
  <td class="text-center">
    <%= check_show_job_exception apply_status.apply.job %>
  </td>
  <td class="text-center">
    <%= apply_status.apply.information[:phone] %>
  </td>
  <td class="text-center">
    <%= show_status_apply apply_status.status_step %>
  </td>
  <td class="hidden-xs text-center">
    <%= link_to employers_job_apply_path(apply_status.job, id: apply_status.apply_id),
      class: "btn btn-default btn-xs" do %>
      <i class="fa fa-eye"></i>
    <% end %>
  </td>
</tr>


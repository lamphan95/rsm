<% if params[:q] %>
  <% if params[:role] == Settings.role.undefined_job %>
    $('#search-job').html('<%= j render "choose_job_undefined_job" %>');
  <% else %>
    $('#search-job').html('<%= j render "choose_jobs" %>');
  <% end %>
  <% choosen_ids = params[:choosen_ids].split(",") %>
  <% if choosen_ids.present? %>
    <% choosen_ids.each do |id| %>
      <% next if id.blank? %>
      $('#job_ids_<%= id %>').prop('checked', true);
    <% end %>
  <% end %>
<% else %>
  $('#add-apply').html('<%= j render "apply_add" %>');
  $('#add-apply').modal('show');
<% end %>
